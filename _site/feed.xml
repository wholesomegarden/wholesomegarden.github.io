  &lt;div class=&quot;mySlides fade&quot;&gt;
    &lt;div class=&quot;numbertext&quot;&gt;7 / 9&lt;/div&gt;
    &lt;img src=&quot;/assets/img/mini-projects/slideshow/ppt-exp-7.gif&quot; style=&quot;width:100%&quot; /&gt;
